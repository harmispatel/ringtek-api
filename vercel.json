{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api-docs",
            "dest": "/server.js",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/api/(.*)",
            "dest": "/server.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, PATCH, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type, Authorization"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/server.js",
            "methods": [
                "GET"
            ]
        }
    ]
}